{
  "name": "pnnl-react-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for React Projects at PNNL",
  "main": "''",
  "scripts": {
    "build": "webpack --config webpack.production.config.js --progress --profile --colors",
    "build-dev": "webpack --progress --profile --colors",
    "start": "webpack-dev-server --progress --profile --colors",
    "watch": "webpack-dev-server --watch --progress --colors --inline --hot --devtool inline-source-map",
    "watch-production": "webpack-dev-server --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "dev": " webpack-dashboard -- webpack-dev-server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "precommit": "npm run lint:staged",
    "lint:staged": "lint-staged"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^2.0.1",
    "i": "^0.3.6",
    "immutable": "^3.8.1",
    "lodash": "4.17.4",
    "memoize": "^0.1.1",
    "moment": "^2.22.2",
    "node-sass": "^4.7.2",
    "npm": "^6.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-table": "^6.8.6",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sass-loader": "^6.0.2",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.22.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "^4.13.1",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.26.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "lint-staged": "^6.0.0",
    "loglevel": "^1.6.0",
    "postcss-loader": "^2.0.9",
    "react-test-renderer": "16.2.0",
    "redux-devtools-extension": "^2.13.2",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.9.7"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "src",
      "src/js",
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/src/shim.js",
      "<rootDir>/src/setupTests.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote",
      "git add"
    ]
  }
}
