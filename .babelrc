{
  "env": {
    "test": {
      "presets": [
        "env",
        {
          "targets": {
            "browsers": ["last 2 Chrome versions"]
          }
        },
        "react",
        "stage-0"
      ],
      "plugins": [
        [
          "transform-runtime",
          {
            "polyfill": false,
            "regenerator": true
          }
        ],
        "transform-async-generator-functions",
        "transform-class-properties",
        "add-module-exports"
      ]
    }
  }
}
